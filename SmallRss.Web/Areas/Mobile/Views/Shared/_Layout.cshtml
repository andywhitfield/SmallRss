<!DOCTYPE html>
<html lang="en">
    <head>
        <meta charset="utf-8" />
        <meta name="viewport" content="width=device-width, initial-scale=1">
        <link rel="shortcut icon" href="@Url.Content("~/mob/image/favicon.ico")" type="image/x-icon" />
        <title>SmallRss</title>
        <script type="text/javascript">
            var urls = {
                feedstatus_api: '/api/feedstatus', // '@Url.Action("feedstatus", "api")',
                article_api: '/api/article', // '@Url.Action("article", "api")',
                feed_api: '/api/feed', // '@Url.Action("feed", "api")'
            };
        </script>

        @Styles.Render("~/mob/css/allcss")
        @Scripts.Render("~/mob/js/alljs")
    </head>
    <body>
        <header>
            <span><a href="@Url.Action("", "")"><img src="@Url.Content("~/mob/image/smallrss.png")" width="28" height="28" alt="SmallRss" />SmallRss</a></span>
            @if (User != null && User.Identity.IsAuthenticated) {
            <span class="manage"><a href="@Url.Content("~/home/logout")">Logout</a></span>
            }
            <span>Manage Feeds</span>
        </header>

        @RenderBody()

        @RenderSection("scripts", required: false)
    </body>
</html>
